{
  "name": "react-starterpack",
  "version": "1.0.0",
  "description": "",
  "main": "server/lib/index.js",
  "scripts": {
    "build:client": "webpack",
    "build:server": "babel ./server/src -d ./server/lib",
    "dev": "source ENV_VAR && concurrently -k \"npm run watch:client\" \"npm run watch:server\" \"nodemon --watch server/lib --delay 1\"",
    "prepare": "npm run build:server",
    "start": "npm run build:client && npm run build:server && npm run start:server",
    "start:server": "node server/lib/index.js",
    "watch:client": "webpack --watch",
    "watch:server": "babel ./server/src -w -d ./server/lib"
  },
  "author": "Luke Robinson",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "mailgun-js": "^0.16.0",
    "morgan": "^1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-stripe-elements": "^1.6.0",
    "stripe": "^5.6.1",
    "uuid": "^3.2.1",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "concurrently": "^3.5.1",
    "file-loader": "^1.1.11",
    "nodemon": "^1.17.2",
    "url-loader": "^1.0.1"
  }
}
